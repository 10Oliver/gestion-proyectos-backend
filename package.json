{
  "name": "gestion-proyectos-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write .",
    "test": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test vitest run",
    "test:watch": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test vitest",
    "db:migrate": "cross-env NODE_OPTIONS=\"-r ts-node/register\" sequelize-cli db:migrate",
    "db:migrate:undo": "cross-env NODE_OPTIONS=\"-r ts-node/register\" sequelize-cli db:migrate:undo",
    "db:seed": "cross-env NODE_OPTIONS=\"-r ts-node/register\" sequelize-cli db:seed:all",
    "db:seed:undo": "cross-env NODE_OPTIONS=\"-r ts-node/register\" sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "dotenv-safe": "^9.1.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/dotenv-safe": "^9.1.0",
    "@types/express": "^5.0.5",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.6.2",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vitest": "^4.0.10"
  }
}
